{
  "name": "qdb-grafana-datasource",
  "version": "3.8.2",
  "description": "QuasarDB datasource for Grafana",
  "main": "src/module.js",
  "author": "Mike Jackson <mike@quasardb.net>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.24.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^7.1.2",
    "prettier": "^1.18.2",
    "q": "^1.5.1",
    "rimraf": "^2.6.2",
    "regenerator-runtime": "^0.13.0"
  },
  "scripts": {
    "start": "npm run build:rm && concurrently \"npm run build:cp --watch\" \"npm run build:js --watch\"",
    "build": "npm run build:rm && concurrently \"npm run build:cp\" \"npm run build:readme\" \"npm run build:js\"",
    "build:rm": "rimraf ./dist && mkdir dist ",
    "build:cp": "cpx -v \"./src/{img/**/*,partials/**/*,plugin.json}\" ./dist",
    "build:readme": "cpx -v \"README.md\" ./dist",
    "build:js": "cross-env NODE_ENV=production BABEL_ENV=production babel ./src -d ./dist --source-maps",
    "lint": "eslint . --cache",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test mocha --require babel-core/register ./test"
  }
}