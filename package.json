{
  "name": "grafana-quasar-datasource",
  "version": "1.0.0",
  "description": "QuasarDB datasource for Grafana",
  "main": "src/module.js",
  "author": "Remi de Groot <rdg94@live.nl>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "start": "yarn build --watch",
    "build": "rimraf ./dist && mkdir dist && yarn build:cp && yarn build:js",
    "build:cp": "cp -r ./src/img ./src/partials ./src/plugin.json ./dist/",
    "build:js": "cross-env NODE_ENV=production && babel ./src -d ./dist",
    "lint": "eslint . --cache"
  }
}
